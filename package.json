{
  "name": "cmc-go",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch server/_core/index.ts",
    "dev:e2e": "cross-env NODE_ENV=development tsx server/_core/index.ts",
    "build": "vite build && esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node scripts/run-migrations.mjs",
    "db:push:dev": "drizzle-kit push",
    "db:push:yes": "node scripts/db-push-yes.mjs",
    "db:init": "node scripts/init-mysql-db.mjs",
    "db:seed": "tsx scripts/seed-database.mjs",
    "db:seed:baseline": "tsx scripts/seed-baseline.mjs",
    "db:setup": "node scripts/init-mysql-db.mjs && pnpm db:push:yes && pnpm db:seed",
    "db:check": "node scripts/check-schema.mjs",
    "db:audit": "node scripts/audit-migrations.mjs",
    "db:verify": "node scripts/verify-write.mjs",
    "db:reset": "node scripts/reset-db.mjs",
    "db:reset:drop": "node scripts/reset-db.mjs --drop-db",
    "db:reset:local": "node scripts/reset-local-db.mjs",
    "validate:agents": "node scripts/validate-agents.mjs",
    "agent:check": "pnpm check && pnpm test && pnpm validate:agents",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
